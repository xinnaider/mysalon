<Page x:Class="wpf_sallonnovo.Views.Pages.AgendamentoSalao"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:wpf_sallonnovo.Views.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="720" d:DesignWidth="980"
      Title="AgendamentoSalao">

    <Grid Background="White">
        
        <ScrollViewer CanContentScroll="True" Width="960" Height="620" HorizontalAlignment="Left">
            <StackPanel Margin="20 50 20 50" x:Name="listAgenda" HorizontalAlignment="Stretch" />
        </ScrollViewer>
        <Button Width="80" Height="30" HorizontalAlignment="Right" Margin="0 10 50 0" VerticalAlignment="Top" x:Name="btnVoltar" Click="btnVoltar_Click">Voltar</Button>
    </Grid>
</Page>